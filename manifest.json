{
	"manifest_version": 2,
	"name": "Quick Note",
	"description": "Easy note taking.",
	"version": "1.0",
    "chrome_url_overrides" : {
      "newtab" : "notes.html"
    },
	"permissions": [
		"storage"
	],
	"icons": { 
		"16": "img/icon16.png",
		"48": "img/icon48.png",
		"128": "img/icon128.png" 
	},
    "browser_action": {
      "default_icon": "img/icon16.png",
      "default_popup": "popup.html"
    },
	"web_accessible_resources": [
		"img/icon128.png"
	]
}
